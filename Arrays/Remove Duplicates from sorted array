class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        
        int left = 0;
        int right = 0;
        int count=0;
        for(;right<nums.size();right++)
        {
            if(nums[left]!=nums[right])
                {
                    swap(nums[left+1],nums[right]);
                    left++;
                    count++;
                }
        }
        return count+1;
    }
};

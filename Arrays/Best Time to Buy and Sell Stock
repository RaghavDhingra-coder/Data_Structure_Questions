class Solution {
public:
    int maxProfit(vector<int>& nums) {
        
        int max_profit = 0;
        int buy = nums[0];

        for(int i=1; i<nums.size(); i++)
        {
            max_profit = max(max_profit,nums[i]-buy);

            buy = min(buy,nums[i]);
        }

        return max_profit;
    }
};

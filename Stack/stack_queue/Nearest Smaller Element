vector<int> Solution::prevSmaller(vector<int> &A) {
    
    stack<int>stk;
    
    vector<int>ans(A.size());
    
    for(int i=0; i<A.size(); i++)
    {
        while(stk.size()!=0 && stk.top() >= A[i])
        {
            stk.pop();
        }
        
        if(stk.size() == 0)
        {
            ans[i] = -1;
        }
        else
        {
            ans[i] = stk.top();
        }
        stk.push(A[i]);
    }
    
    return ans;
}
